{
	"luci-app-w1700k-fancontrol": {
		"description": "Grant access to W1700K fan control",
		"read": {
			"ubus": {
				"luci.fan": [ "getStatus", "getCurve", "getAllCurves" ]
			},
			"uci": [ "fan" ],
			"file": {
				"/sys/class/hwmon/hwmon5/*": [ "read" ]
			}
		},
		"write": {
			"ubus": {
				"luci.fan": [ "setMode", "setManualPwm", "setPreset", "setCustomCurve" ]
			},
			"uci": [ "fan" ]
		}
	}
}
